    "uuConsoleCommandlineTemplateCreate": "uu_console_commandlineg01-devkit templateCreate",
    "uuCloudg02AppBoxCopy": "uu_cloudg02-devkit appBoxCopy",
    "uuCloudg02AppBox": "uu_cloudg02-devkit appBox",
